<h2>Championship > Rounds</h2>

<%= form_for [@championship, @round] do |f| %>
    <%= f.label :date %>
    <%= f.text_field :date %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :track %>
    <%= f.select :track, options_from_collection_for_select(@available_tracks, :id, :name) %>

    <%= render :partial => 'roundPosition', :collection => @drivers, :as => :driver, :locals => { :f => f } %>
    <%= f.submit %>
<% end %>
